{
  "description": "Veja quem n√£o me seguiu de volta no Instagram | See who hasn't followed me back on Instagram",
  "manifest_version": 2,
  "name": "Instragram Unfollow",
  "author": "Johnendz",
  "version": "1.2",

  "content_scripts": [
    {
      "matches": ["*://www.instagram.com/*"],
      "exclude_matches": ["*://www.instagram.com/developer/*"],
	  "js":["script.js"],
	  "run_at": "document_start"
    }
  ],
  
  "icons": {
	"32": "icons/32.png",
	"64": "icons/64.png",
	"128": "icons/128.png"
  }
}